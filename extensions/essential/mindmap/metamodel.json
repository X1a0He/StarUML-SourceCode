{
  "MMElement": {
    "kind": "class",
    "super": "ExtensibleModel"
  },
  "MMMindmap": {
    "kind": "class",
    "super": "MMElement"
  },
  "MMMindmapDiagram": {
    "kind": "class",
    "super": "Diagram",
    "views": [
      "UMLCustomTextView",
      "ShapeView",
      "UMLNoteLinkView",
      "MMNodeView"
    ]
  },
  "MMNode": {
    "kind": "class",
    "super": "MMElement",
    "view": "MMNodeView"
  },
  "MMEdge": {
    "kind": "class",
    "super": "DirectedRelationship",
    "view": "MMEdgeView"
  },
  "MMNodeView": {
    "kind": "class",
    "super": "NodeView",
    "attributes": [
      { "name": "nameLabel", "kind": "ref",  "type": "LabelView", "embedded": "subViews" },
      { "name": "wordWrap",  "kind": "prim", "type": "Boolean", "default": true }
    ]
  },
  "MMEdgeView": {
    "kind": "class",
    "super": "EdgeView"
  }
}
