{
  "components": [
    {
      "id": "wireframe.set-name",
      "type": "input",
      "hint": "name",
      "property": "name",
      "command": "engine:set-property"
    },
    {
      "id": "wireframe.add-tab",
      "type": "button",
      "icon": "icon-tool-WFAddTab",
      "hint": "Add Tab",
      "command": "wireframe:add-tab"
    }
  ],
  "quickedits": [
    {
      "id": "wireframe.common",
