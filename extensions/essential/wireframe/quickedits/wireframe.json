      "components": [
        { "id": "wireframe.set-name", "section": "main" },
        { "id": "uml.add-note", "section": "left"}
      ]
    },
    {
      "id": "wireframe.tab-list",
      "targets": [
        "WFTabListView"
      ],
      "area": { "x1": "left", "y1": "top", "x2": "right", "y2": "bottom" },
      "components": [
        { "id": "uml.add-note", "section": "left"},
        { "id": "wireframe.add-tab", "section": "right" }
      ]
    }
  ]
