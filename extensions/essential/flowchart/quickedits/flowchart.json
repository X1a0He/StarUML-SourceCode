    {
      "id": "fc.add-incoming-process",
      "type": "button",
      "icon": "icon-tool-fc-incoming-process",
      "hint": "Add Incoming Process",
      "command": "fc:add-incoming-process"
    },
    {
      "id": "fc.add-outgoing-decision",
      "type": "button",
      "icon": "icon-tool-fc-outgoing-decision",
      "hint": "Add Outgoing Decision",
      "command": "fc:add-outgoing-decision"
    },
    {
      "id": "fc.add-incoming-decision",
      "type": "button",
      "icon": "icon-tool-fc-incoming-decision",
      "hint": "Add Incoming Decision",
      "command": "fc:add-incoming-decision"
    }
