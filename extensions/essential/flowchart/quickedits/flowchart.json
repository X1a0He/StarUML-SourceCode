        "FCPreparationView",
        "FCDisplayView",
        "FCManualInputView",
        "FCManualOperationView",
        "FCCardView",
        "FCPunchedTapeView",
        "FCConnectorView",
        "FCOffPageConnectorView",
        "FCOrView",
        "FCSummingJunctionView",
        "FCCollateView",
        "FCSortView",
